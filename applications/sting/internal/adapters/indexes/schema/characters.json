{
  "settings": {
    "analysis": {
      "analyzer": {
        "autocomplete": {
          "tokenizer": "autocomplete",
          "filter": [
            "lowercase",
            "truncate"
          ]
        },
        "autocomplete_search": {
          "tokenizer": "lowercase"
        }
      },
      "tokenizer": {
        "autocomplete": {
          "type": "edge_ngram",
          "min_gram": 2,
          "max_gram": 25,
          "token_chars": [
            "letter"
          ]
        }
      }
    }
  },
  "mappings": {
    "dynamic": "strict",
    "properties": {
      "id": {
        "type": "keyword"
      },
      "name": {
        "properties": {
          "language": {
            "type": "keyword"
          },
          "supported": {
            "type": "boolean"
          },
          "en": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "slug": {
        "type": "keyword"
      },
      "thumbnail_resource": {
        "type": "keyword"
      },
      "banner_resource": {
        "type": "keyword"
      },
      "created_at": {
        "type": "date"
      },
      "total_likes": {
        "type": "integer"
      },
      "total_posts": {
        "type": "integer"
      },
      "club_id": {
        "type": "keyword"
      },
      "series": {
        "type": "nested",
        "properties": {
          "id": {
            "type": "keyword"
          },
          "slug": {
            "type": "keyword"
          },
          "thumbnail_resource": {
            "type": "keyword"
          },
          "banner_resource": {
            "type": "keyword"
          },
          "total_likes": {
            "type": "integer"
          },
          "total_posts": {
            "type": "integer"
          },
          "title": {
            "properties": {
              "language": {
                "type": "keyword"
              },
              "supported": {
                "type": "boolean"
              },
              "en": {
                "type": "text",
                "analyzer": "autocomplete",
                "search_analyzer": "autocomplete_search"
              }
            }
          },
          "created_at": {
            "type": "date"
          },
          "updated_at": {
            "type": "date"
          }
        }
      },
      "updated_at": {
        "type": "date"
      }
    }
  }
}
