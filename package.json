  "scripts": {
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "check:tsc": "tsc -b",
    "check:eslint": "eslint --quiet -c .eslintrc.json ./applications/medusa/src ./applications/medusa/cypress",
    "docs": "docsify serve docs",
    "locale:extract": " cd ./applications/medusa && lingui extract",
    "locale:compile": "cd ./applications/medusa && lingui compile",
    "service:medusa:e2e": "wait-port --wait-for-dns eva:8000 && wait-port --wait-for-dns sting:8000 && wait-port --wait-for-dns parley:8000 && wait-port --wait-for-dns puppy:8000 && cd ./applications/medusa && URL=https://traefik CLIENT_PUBLIC_PATH=https://traefik/ REMOTE_DEV=true razzle start",
    "relay": "cd ./applications/medusa && concurrently --raw --kill-others \"node relay-local-persisting.js\" \"relay-compiler --watch\" ",
    "graphql": "cd ./applications/medusa && node get-graphql-schema.js -r dotenv/config",
    "analyze": "cd ./applications/medusa && ANALYZE_BUNDLE=true razzle build --noninteractive",
    "medusa:build": "cd ./applications/medusa && razzle build --noninteractive",
    "proxy:medusa:local": "telepresence --swap-deployment medusa --expose 8000 --run yarn run service:medusa:local",
    "proxy:medusa:remote": "telepresence --swap-deployment medusa --expose 8000 --run yarn run service:medusa:remote",
    "proxy:medusa:production": "telepresence --swap-deployment medusa --expose 8000 --run yarn run service:medusa:production",
    "service:medusa:remote": "cd ./applications/medusa && REMOTE_DEV=true razzle start",
    "service:medusa:local": "cd ./applications/medusa && URL=https://localhost:3001 CLIENT_PUBLIC_PATH=https://localhost:3001/ razzle start",
    "service:medusa:production": "cd ./applications/medusa && node build/server.js",
    "storybook": "cd ./applications/medusa && start-storybook -p 6006 -s public,../../development/assets",
    "test:unit": "bazel coverage --test_output=all //applications/medusa:unit",
    "e2e:open": "cd ./applications/medusa && cypress open --config-file cypress.json",
    "jest": "cd ./applications/medusa && jest src/ --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
