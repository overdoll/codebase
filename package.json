{
  "name": "001",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "yarn run relay; concurrently --kill-others \"yarn run start:server\" \"yarn run start:client\" \"yarn run relay --watch\"",
    "start:client": "PORT=3001 react-scripts start",
    "start:server": "NODE_ENV=development node server",
    "start:prod": "react-scripts build && NODE_ENV=production node server",
    "build": "yarn run relay && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "relay": "yarn run relay-compiler --schema ./applications/web/schema/schema.graphql --src ./applications/web/src/ $@"
  },
  "dependencies": {
    "express": "^4.14.0",
    "husky": "^4.3.6",
    "ignore-styles": "^5.0.1",
    "import-export": "^1.0.1",
    "lint-staged": "^10.5.3",
    "node-fetch": "^1.6.3",
    "react": "^0.0.0-experimental-4ead6b530",
    "react-dom": "^0.0.0-experimental-4ead6b530",
    "react-relay": "^0.0.0-experimental-0b967d2e",
    "react-router": "^5.2.0",
    "relay-runtime": "^10.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-relay": "^10.1.2",
    "concurrently": "^5.3.0",
    "graphql": "^15.4.0",
    "http-proxy-middleware": "0.17.3",
    "prettier": "^2.2.1",
    "react-scripts": "0.9.5",
    "relay-compiler": "^10.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "ISC"
}
